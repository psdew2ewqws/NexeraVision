  taxableCategories TaxableCategory[]

  @@index([companyId, isActive])
  @@index([companyId, isDefault])
  @@index([taxType, isActive])
  @@map("taxes")
}

// Polymorphic tax associations for products
model TaxableProduct {
  id        String   @id @default(uuid())
  taxId     String   @map("tax_id")
  productId String   @map("product_id")
  createdAt DateTime @default(now()) @map("created_at")

  tax     Tax         @relation(fields: [taxId], references: [id], onDelete: Cascade)
  product MenuProduct @relation(fields: [productId], references: [id], onDelete: Cascade)

  @@unique([taxId, productId])
  @@index([productId])
  @@map("taxable_products")
}

// Polymorphic tax associations for modifiers
model TaxableModifier {
  id         String   @id @default(uuid())
  taxId      String   @map("tax_id")
  modifierId String   @map("modifier_id")
  createdAt  DateTime @default(now()) @map("created_at")

  tax      Tax      @relation(fields: [taxId], references: [id], onDelete: Cascade)
  modifier Modifier @relation(fields: [modifierId], references: [id], onDelete: Cascade)

  @@unique([taxId, modifierId])
  @@index([modifierId])
  @@map("taxable_modifiers")
}

// Polymorphic tax associations for categories
model TaxableCategory {
  id         String   @id @default(uuid())
  taxId      String   @map("tax_id")
  categoryId String   @map("category_id")
  createdAt  DateTime @default(now()) @map("created_at")

  tax      Tax          @relation(fields: [taxId], references: [id], onDelete: Cascade)
  category MenuCategory @relation(fields: [categoryId], references: [id], onDelete: Cascade)

  @@unique([taxId, categoryId])
  @@index([categoryId])
  @@map("taxable_categories")
}

// Company tax configuration
model CompanyTaxSetting {
  id        String @id @default(uuid())
  companyId String @unique @map("company_id")

  // Tax display preferences
  priceDisplayMode TaxDisplayMode  @default(tax_inclusive) @map("price_display_mode")
  taxRoundingMode  TaxRoundingMode @default(round_half_up) @map("tax_rounding_mode")
  decimalPlaces    Int             @default(2) @map("decimal_places")

  // Receipt display options
  showTaxBreakdown     Boolean @default(true) @map("show_tax_breakdown")
  showTaxInclusiveText Boolean @default(true) @map("show_tax_inclusive_text")
  taxLineLabel         Json    @default("{\"en\": \"Tax\", \"ar\": \"الضريبة\"}") @map("tax_line_label")
  vatNumberLabel       Json    @default("{\"en\": \"VAT Number\", \"ar\": \"الرقم الضريبي\"}") @map("vat_number_label")

  // Company tax registration info
  taxRegistrationNumber String? @map("tax_registration_number")
  vatNumber             String? @map("vat_number")

  // Default tax settings
  defaultTaxId        String? @map("default_tax_id")
  autoApplyDefaultTax Boolean @default(false) @map("auto_apply_default_tax")

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  company Company @relation(fields: [companyId], references: [id], onDelete: Cascade)

  @@index([companyId])
  @@map("company_tax_settings")
}

// ═══════════════════════════════════════════════════════════════════════════
// CUSTOMER MODELS - For delivery orders from external providers
// ═══════════════════════════════════════════════════════════════════════════

model Customer {
  id           String    @id @default(uuid())
  companyId    String    @map("company_id")
  name         String
  phone        String
  email        String?
  language     String    @default("en")
  totalOrders  Int       @default(0) @map("total_orders")
  totalSpent   Decimal   @default(0) @map("total_spent") @db.Decimal(10, 2)
  lastOrderAt  DateTime? @map("last_order_at")
  createdAt    DateTime  @default(now()) @map("created_at")
  updatedAt    DateTime  @updatedAt @map("updated_at")

  company   Company           @relation(fields: [companyId], references: [id], onDelete: Cascade)
  addresses CustomerAddress[]
  orders    Order[]

  @@unique([companyId, phone])
  @@index([companyId])
  @@index([phone])
  @@map("customers")
}

model CustomerAddress {
  id          String   @id @default(uuid())
  customerId  String   @map("customer_id")
  companyId   String   @map("company_id")
  addressType String   @default("delivery") @map("address_type") // 'delivery', 'billing'
  street      String
  building    String?
  floor       String?
  apartment   String?
  city        String
  area        String?
  landmark    String?
  latitude    Decimal? @db.Decimal(10, 8)
  longitude   Decimal? @db.Decimal(11, 8)
  notes       String?  @db.Text
  isDefault   Boolean  @default(false) @map("is_default")
  createdAt   DateTime @default(now()) @map("created_at")
  updatedAt   DateTime @updatedAt @map("updated_at")

  customer Customer @relation(fields: [customerId], references: [id], onDelete: Cascade)
  company  Company  @relation(fields: [companyId], references: [id], onDelete: Cascade)

  @@index([customerId])
  @@index([companyId])
  @@map("customer_addresses")
}

// ═══════════════════════════════════════════════════════════════════════════
// DELIVERY INTEGRATION MODELS - Production-Grade Integration System
// ═══════════════════════════════════════════════════════════════════════════

// Delivery Provider Configuration
model DeliveryProvider {
  id               String  @id @default(uuid())
  code             String  @unique // 'careem', 'talabat', 'ubereats'
  name             String // Display name
  description      String? @db.Text
  apiBaseUrl       String? @map("api_base_url")
  webhookEndpoint  String? @map("webhook_endpoint") // Our endpoint for this provider
  isActive         Boolean @default(false) @map("is_active")
  supportsWebhooks Boolean @default(true) @map("supports_webhooks")
  supportsApi      Boolean @default(false) @map("supports_api")

  // Provider-specific configuration (stored as JSON)
  config Json? // API keys, secrets, URLs, etc.

  // Rate limiting configuration
  rateLimitPerMinute Int @default(100) @map("rate_limit_per_minute")

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  // Relations
  branchConfigs          BranchDeliveryConfig[]
  webhookLogs            WebhookLog[]
  providerOrderLogs      ProviderOrderLog[]
  deliveryErrorLogs      DeliveryErrorLog[]
  companies              Company[]
  orders                 Order[]
  deliveryProviderOrders DeliveryProviderOrder[]

  @@index([code, isActive])
  @@map("delivery_providers")
}

// Branch-specific delivery configuration
model BranchDeliveryConfig {
  id         String @id @default(uuid())
  branchId   String @map("branch_id")
  providerId String @map("provider_id")
  companyId  String @map("company_id") // For data isolation

  // Provider-specific merchant configuration
  merchantId    String? @map("merchant_id") // Provider's merchant ID
  storeId       String? @map("store_id") // Provider's store ID
  apiKey        String? @map("api_key") // Branch-specific API key
  apiSecret     String? @map("api_secret") // Branch-specific secret
  webhookSecret String? @map("webhook_secret") // For signature validation

  // Integration settings
  isActive           Boolean @default(false) @map("is_active")
  autoAcceptOrders   Boolean @default(false) @map("auto_accept_orders")
  autoPrintOnReceive Boolean @default(true) @map("auto_print_on_receive")
  syncMenu           Boolean @default(false) @map("sync_menu")

  // Provider-specific settings (JSON)
  settings Json? // Additional provider-specific settings

  // Metadata
  lastSyncAt DateTime? @map("last_sync_at")
  createdAt  DateTime  @default(now()) @map("created_at")
  updatedAt  DateTime  @updatedAt @map("updated_at")
  createdBy  String?   @map("created_by")
  updatedBy  String?   @map("updated_by")

  // Relations
  branch   Branch           @relation(fields: [branchId], references: [id], onDelete: Cascade)
  provider DeliveryProvider @relation(fields: [providerId], references: [id])
  company  Company          @relation(fields: [companyId], references: [id], onDelete: Cascade)

  @@unique([branchId, providerId])
  @@index([companyId, isActive])
  @@index([branchId, isActive])
  @@index([providerId, isActive])
  @@map("branch_delivery_configs")
}

// Webhook log for tracking all incoming webhooks
model WebhookLog {
  id         String  @id @default(uuid())
  providerId String  @map("provider_id")
  branchId   String? @map("branch_id")
  companyId  String? @map("company_id")

  // Webhook details
  webhookType     String  @map("webhook_type") // 'order_created', 'status_update', etc.
  externalOrderId String? @map("external_order_id") // Provider's order ID
  internalOrderId String? @map("internal_order_id") // Our order ID

  // Request data
  endpoint  String
  method    String  @default("POST")
  headers   Json?
  payload   Json // Raw webhook payload
  signature String? // For signature validation
  ipAddress String? @map("ip_address") // Client IP address

  // Processing status
  status      WebhookStatus @default(pending)
  processedAt DateTime?     @map("processed_at")
  retryCount  Int           @default(0) @map("retry_count")
  maxRetries  Int           @default(10) @map("max_retries")
  nextRetryAt DateTime?     @map("next_retry_at")

  // Response/Error tracking
  response     Json? // Response sent back
  errorMessage String? @map("error_message") @db.Text
  errorStack   String? @map("error_stack") @db.Text

  // Timestamps
  receivedAt DateTime @default(now()) @map("received_at")
  createdAt  DateTime @default(now()) @map("created_at")
  updatedAt  DateTime @updatedAt @map("updated_at")

  // Relations
  provider DeliveryProvider @relation(fields: [providerId], references: [id])
  branch   Branch?          @relation(fields: [branchId], references: [id], onDelete: SetNull)
  company  Company?         @relation(fields: [companyId], references: [id], onDelete: SetNull)
  order    Order?           @relation(fields: [internalOrderId], references: [id], onDelete: SetNull)

  @@index([providerId, status, nextRetryAt])
  @@index([branchId, status])
  @@index([externalOrderId])
  @@index([internalOrderId])
  @@index([status, retryCount])
  @@index([createdAt])
  @@map("webhook_logs")
}

// Provider order tracking log
model ProviderOrderLog {
  id         String @id @default(uuid())
  orderId    String @map("order_id")
  providerId String @map("provider_id")
  branchId   String @map("branch_id")
  companyId  String @map("company_id")

  // Provider order details
  externalOrderId String       @map("external_order_id")
  providerStatus  String       @map("provider_status") // Provider's status value
  mappedStatus    OrderStatus? @map("mapped_status") // Our mapped status

  // Delivery details
  deliveryType      String?   @map("delivery_type") // 'careem', 'talabat_delivery', etc.
  estimatedDelivery DateTime? @map("estimated_delivery")
  actualDelivery    DateTime? @map("actual_delivery")

  // Captain/Driver details
  captainName  String? @map("captain_name")
  captainPhone String? @map("captain_phone")
  captainId    String? @map("captain_id")

  // Financial
  deliveryFee   Decimal? @map("delivery_fee") @db.Decimal(10, 2)
  serviceFee    Decimal? @map("service_fee") @db.Decimal(10, 2)
  totalAmount   Decimal? @map("total_amount") @db.Decimal(10, 2)
  paymentMethod String?  @map("payment_method")
  isPrepaid     Boolean  @default(false) @map("is_prepaid")

  // Raw data
  rawData Json? @map("raw_data") // Complete provider response

  // Timestamps
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  // Relations
  order    Order            @relation(fields: [orderId], references: [id], onDelete: Cascade)
  provider DeliveryProvider @relation(fields: [providerId], references: [id])
  branch   Branch           @relation(fields: [branchId], references: [id])
  company  Company          @relation(fields: [companyId], references: [id])

  @@unique([orderId, providerId])
  @@index([externalOrderId])
  @@index([branchId, providerId])
  @@index([companyId])
  @@index([createdAt])
  @@map("provider_order_logs")
}

// Delivery error tracking for debugging
model DeliveryErrorLog {
  id           String  @id @default(uuid())
  providerId   String  @map("provider_id")
  branchId     String? @map("branch_id")
  companyId    String? @map("company_id")
  webhookLogId String? @map("webhook_log_id")
  orderId      String? @map("order_id")

  // Error details
  errorType    String  @map("error_type") // 'validation', 'processing', 'network', etc.
  errorCode    String? @map("error_code")
  errorMessage String  @map("error_message") @db.Text
  errorStack   String? @map("error_stack") @db.Text
  errorDetails Json?   @map("error_details") // Detailed error information

  // Context
  context      Json? // Additional context for debugging
  requestData  Json? @map("request_data")
  responseData Json? @map("response_data")

  // Retry tracking
  retryCount          Int       @default(0) @map("retry_count")
  nextRetryAt         DateTime? @map("next_retry_at")
  movedToDeadLetterAt DateTime? @map("moved_to_dead_letter_at")

  // Resolution
  isResolved      Boolean   @default(false) @map("is_resolved")
  resolvedAt      DateTime? @map("resolved_at")
  resolvedBy      String?   @map("resolved_by")
  resolutionNotes String?   @map("resolution_notes") @db.Text

  // Timestamps
  occurredAt DateTime @default(now()) @map("occurred_at")
  createdAt  DateTime @default(now()) @map("created_at")
  updatedAt  DateTime @updatedAt @map("updated_at")

  // Relations
  provider DeliveryProvider @relation(fields: [providerId], references: [id])
  branch   Branch?          @relation(fields: [branchId], references: [id], onDelete: SetNull)
  company  Company?         @relation(fields: [companyId], references: [id], onDelete: SetNull)

  @@index([providerId, errorType, isResolved])
  @@index([branchId, isResolved])
  @@index([occurredAt])
  @@index([nextRetryAt, retryCount])
  @@map("delivery_error_logs")
}

// Enum for webhook processing status
enum WebhookStatus {
  received
  pending
  processing
  completed
  failed
  retrying
  ignored
}
