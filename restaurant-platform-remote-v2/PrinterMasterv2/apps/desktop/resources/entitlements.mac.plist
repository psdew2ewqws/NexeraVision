<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow network access for printer discovery -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Allow network server for local API -->
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- Allow USB access for printer management -->
    <key>com.apple.security.device.usb</key>
    <true/>
    
    <!-- Allow Bluetooth for wireless printer discovery -->
    <key>com.apple.security.device.bluetooth</key>
    <true/>
    
    <!-- Allow file access for printer drivers and configs -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    
    <!-- Allow camera access if printer has scan functionality -->
    <key>com.apple.security.device.camera</key>
    <true/>
    
    <!-- Allow audio-visual access for printer status notifications -->
    <key>com.apple.security.device.audio-input</key>
    <true/>
    
    <!-- Allow accessing system preferences -->
    <key>com.apple.security.automation.apple-events</key>
    <true/>
    
    <!-- JIT compilation for better performance -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Allow unsigned executable memory -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Disable library validation for third-party printer drivers -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow DYLD environment variables -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
    
    <!-- Print access -->
    <key>com.apple.security.print</key>
    <true/>
    
    <!-- Address book access for customer info -->
    <key>com.apple.security.personal-information.addressbook</key>
    <true/>
</dict>
</plist>