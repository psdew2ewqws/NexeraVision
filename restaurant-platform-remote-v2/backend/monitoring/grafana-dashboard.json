{
  "dashboard": {
    "id": null,
    "uid": "restaurant-platform-monitoring",
    "title": "Restaurant Platform - Production Monitoring",
    "tags": ["restaurant", "printing", "websocket", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "panels": [
      {
        "id": 1,
        "title": "WebSocket Connections (Total)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(websocket_connections_total)",
            "legendFormat": "Total Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Correlation ID Success Rate",
        "type": "gauge",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "(correlation_id_cache_hits_total / (correlation_id_cache_hits_total + correlation_id_cache_misses_total)) * 100",
            "legendFormat": "Cache Hit Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            },
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "title": "Print Test Success Rate",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "(sum(print_test_requests_total{status=\"success\"}) / sum(print_test_requests_total)) * 100",
            "legendFormat": "Success Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            },
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        }
      },
      {
        "id": 4,
        "title": "Desktop App Connections",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(desktop_app_connections_total)",
            "legendFormat": "Connected Apps",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Print Test Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(print_test_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(print_test_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(print_test_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [1], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High p95 Latency Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 6,
        "title": "Connection Quality Distribution",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "connection_quality_distribution",
            "legendFormat": "{{quality}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "pieType": "pie"
        }
      },
      {
        "id": 7,
        "title": "WebSocket Connections per Branch",
        "type": "graph",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "websocket_connections_total",
            "legendFormat": "{{branch}} - {{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Connections"
          }
        ]
      },
      {
        "id": 8,
        "title": "Error Rate (Requests/min)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_request_errors_total[1m])) * 60",
            "legendFormat": "Errors per minute",
            "refId": "A"
          },
          {
            "expr": "sum(rate(print_jobs_failed_total[1m])) * 60",
            "legendFormat": "Print failures per minute",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Errors/min"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "1m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 9,
        "title": "Health Monitoring Quality Score",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "avg(health_monitoring_quality_score) by (branch)",
            "legendFormat": "{{branch}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Quality Score (0-100)",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 10,
        "title": "Rate Limiting Activity",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(rate_limit_triggered_total[5m])",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "rate_limit_active_clients",
            "legendFormat": "Active Limited Clients",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Rate"
          }
        ]
      },
      {
        "id": 11,
        "title": "Security Events",
        "type": "graph",
        "gridPos": {"x": 0, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(auth_failures_total[5m])) by (reason)",
            "legendFormat": "Auth Failures - {{reason}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(cors_violations_total[5m]))",
            "legendFormat": "CORS Violations",
            "refId": "B"
          },
          {
            "expr": "sum(rate(security_events_total[5m])) by (severity)",
            "legendFormat": "Security Events - {{severity}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Events/sec"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [50], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "1m", "now"]},
              "reducer": {"type": "sum"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Authentication Failure Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 12,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (p95)"
          }
        ]
      },
      {
        "id": 13,
        "title": "System Resources",
        "type": "graph",
        "gridPos": {"x": 0, "y": 36, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "process_cpu_user_seconds_total",
            "legendFormat": "CPU User",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 14,
        "title": "Print Jobs (Success vs Failed)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 36, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(print_jobs_success_total[5m]))",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "sum(rate(print_jobs_failed_total[5m]))",
            "legendFormat": "Failed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Jobs/sec"
          }
        ]
      }
    ]
  }
}
